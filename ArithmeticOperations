public class ArithmeticOperations {
    public static void main(String[] args) {
        int num = 10;
        
        num += 5;
        System.out.println("After += 5, num = " + num);
        
        num -= 3;
        System.out.println("After -= 3, num = " + num);
        
        num *= 2;
        System.out.println("After *= 2, num = " + num);
    }
}
